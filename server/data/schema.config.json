{
	"type": "object",
	"properties": {
		"coin": {
			"type": "object",
			"description": "Cryptocurrency to trade (i.e. Etherium)",
			"properties": {
				"shortName": {
					"type": "string"
				},
				"longName": {
					"type": "string"
				},
				"fullName": {
					"type": "string"
				}
			},
			"required": ["shortName", "longName", "fullName"]
		},
		"stableCoin": {
			"type": "object",
			"description": "Stable coin to compare against (Tether recommended)",
			"properties": {
				"shortName": {
					"type": "string"
				},
				"longName": {
					"type": "string"
				},
				"fullName": {
					"type": "string"
				}
			},
			"required": ["shortName", "longName", "fullName"]
		},
		"allocation": {
			"type": "number",
			"description": "Allocation of assets at exchange"
		},
		"tickInterval": {
			"type": "number",
			"description": "Interval (minutes) inbetween each request for current values. Coingecko caching is about 2-3 minutes so anything under that is unnecessary"
		},
		"concurrentOrders": {
			"type": "integer",
			"description": "Max amount of orders at the same time"
		},
		"minInitialValues": {
			"type": "integer",
			"description": "For ROC 14 at least 15 intervals are needed"
		},
		"minAlgorithmValues": {
			"type": "integer",
			"description": "For WMA 10 of ROC 14 at least 25 intervals are needed"
		},
		"longROC": {
			"type": "integer",
			"description": "Usually ROC 14"
		},
		"shortNameROC": {
			"type": "integer",
			"description": "Usually ROC 11"
		},
		"WMA": {
			"type": "integer",
			"description": "Usually WMA 10"
		},
		"buyBuffer": {
			"type": "integer",
			"description": "Finetuning of when to buy"
		},
		"falsePositiveBuffer": {
			"type": "number",
			"description": "Multiplier of 24h price change to avoid buying at false positive (too close to daily high)"
		},
		"sellBuffer": {
			"type": "integer",
			"description": "Finetuning of when to sell"
		},
		"ATRmultiplier": {
			"type": "number",
			"description": "Multiplier of ATR to set sell point"
		}
	},
	"required": [
		"coin",
		"stableCoin",
		"allocation",
		"tickInterval",
		"concurrentOrders",
		"minInitialValues",
		"minAlgorithmValues",
		"longROC",
		"shortNameROC",
		"WMA",
		"buyBuffer",
		"sellBuffer",
		"ATRmultiplier"
	]
}
